"UnlitGeneric"
{
	"$basetexture"	"pb/greenooze2"
	"$alpha"	".6"
	"%compilepassbullets" 1
	"$envmap" "env_cubemap"
	"$envmapcontrast" 0.8
	"$envmapsaturation" 0.5
	"$envmaptint" "[ .5 .9 .5 ]"
	"Proxies"
	{
		"TextureScroll"
		{
			"texturescrollvar" "$basetexturetransform"
			"texturescrollrate" .2
			"texturescrollangle" -90
		}
	}

}
